{
  "extends": [
    "tslint-react",
    "tslint:recommended",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "import-name": false,
    "no-console": false,
    "variable-name": false,
    "jsx-no-lambda": false,
    "interface-name": false
  }
}
